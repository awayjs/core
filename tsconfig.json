{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "lib": ["es6", "ES2015.Symbol","DOM","ScriptHost","ES2015.Iterable"],
    "moduleResolution": "node",
    "noImplicitUseStrict": true,
    "noEmitHelpers": true,
    "importHelpers": true,
    "declaration": true,
    "declarationMap": true,
    "experimentalDecorators": true,
    "outDir": "./dist"
  },
  "exclude": [
    "node_modules",
    "dist",
    "tests",
    "assembly"
  ]
}